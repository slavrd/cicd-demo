{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "S3DeployUser",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObjectAcl",
                "s3:GetObject",
                "s3:AbortMultipartUpload",
                "s3:DeleteObject",
                "s3:PutObjectAcl"
            ],
            "Resource": ["${bucket_list}"]
        }
    ]
}